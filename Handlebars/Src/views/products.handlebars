{{#if products}}
<h1>Listado de Productos</h1>
<div>
<a href="./">
   <button>Cargar un producto nuevo</button>
</a>
 </div>
  {{#each products}}

        <img src="{{this.thumbnail}}"/>
        <h2>{{this.title}}</h2>
        <p>{{this.price}}</p>
 
  {{/each}}
{{else}}
  <h2>No hay productos</h2>
{{/if}}
